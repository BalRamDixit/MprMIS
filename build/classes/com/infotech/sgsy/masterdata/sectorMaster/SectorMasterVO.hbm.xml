<?xml version="1.0" encoding="UTF-8"?>


<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  	
  	<class name="com.infotech.sgsy.masterdata.sectorMaster.SectorMasterVO"  table="mst_sector" schema="public">
	    
	    
         <id name="sectorId" column="sector_id">
    		<generator class="sequence">
        		<param name="sequence">mst_sector_id_seq</param>
    		</generator>
		</id> 
        	   
   			    <property name="sectorCode"   						  type="string"><column name="sector_code" /></property>      
   			    <property name="sectorName"      					  type="string"><column name="sector_name" /></property>    			     
   			    <property name="createdBy"             					  type="string"><column name="created_by" /></property>
                <property name="createdOn"           					  type="date"><column name="created_on" /></property>
                <property name="updatedBy"           					  type="string"><column name="last_modified_by" /></property>
                <property name="updatedOn"             					  type="date"><column name="last_modified_on" /></property>
                
       
  	</class>
  	
  	
</hibernate-mapping>