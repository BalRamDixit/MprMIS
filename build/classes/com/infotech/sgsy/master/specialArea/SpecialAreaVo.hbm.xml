<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.infotech.sgsy.master.specialArea.SpecialAreaVo" table="mst_special_area" schema="public">

		<id name="specialAreaId" column="special_area_id ">
			<generator class="sequence">
				<param name="sequence">mst_special_area_Id_Seq</param>
			</generator>
		</id>

		<property name="specialAreaCode" column="special_area_code" type="string" />
		<property name="specialAreaName" column="special_area_name"  type="string"/>
		

		<property name="createdOn" column="created_on" type="date" length="10" />
		<property name="createdBy" column="created_by" length="25" />
		<property name="updatedOn" column="modified_on" type="date"  length="10" />
		<property name="updateBy" column="modifed_by" length="25" />



	</class>
</hibernate-mapping>