<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping> 

	<class name="com.infotech.sgsy.master.bankLevel.BankLevelVO" table="mst_bank_level"> 
		
		<id name="bankLevelCode" column="bank_level_Code" length="6">		
			<generator class="increment"/>
		</id>	
			   	
			   	
 	  	<property name="bankLevelName" column="bank_Level_Name" not-null="true"  length="40"/>	  	
 	  	<property name="activeFlag" column="activeFlag"  not-null="true"  length="1"/>  	  	 		 	
 		<property name="createdOn" column="createdOn"  /> 
 		<property name="createdBy" column="createdBy" length="25"/> 		
 		<property name="lastModifiedOn" column="lastModifiedOn"  /> 
 		<property name="lastModifedBy" column="lastModifedBy" length="25"/>
 		 
 		 	
 	</class>
 	
</hibernate-mapping>