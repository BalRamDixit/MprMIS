<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.infotech.sgsy.master.tsaMaster.TsaMasterVO"  table="mst_tsa" schema="public">
    
        <id name="id" column="tsa_id">
    		<generator class="sequence">
        		<param name="sequence">mst_tsa_Id_seq</param>
    		</generator>
		</id>
       
       
       
        <property name="tsaName"                type="string"><column name="tsa_name" /></property>
        <property name="tsaCode"                type="string"><column name="tsa_code" /></property>
        <property name="isdDeleted"              type="boolean"><column name="is_deleted" /></property>
   
        
        <property name="createdBy"               type="string"><column name="created_by" /></property>
        <property name="updatedBy"               type="string"><column name="last_modified_by" /></property>
        <property name="createdOnDate"           type="date"><column name="created_on" /></property>
        <property name="updatedOnDate"           type="date"><column name="last_modified_on" /></property>
        
	</class>
</hibernate-mapping>

