<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.infotech.sgsy.projectSetupTarget.ProjectSetupTargetVO"  table="project_target" schema="public">
    
        <id name="id" type="int">
         <column name="id" />
            <generator class="increment">			 	
			</generator>  
        </id>
       
         <property name="projectId"      						  type="string"><column name="project_id" /></property>
         <property name="totalTrainingTarget"   		          type="string"><column name="total_training_target" /></property>
        <property name="scSt"    									  type="string"><column name="scSt" /></property>
         <property name="general"     							  type="string"><column name="general" /></property>
        <property name="minority"   							  type="string"><column name="minority" /></property>
        <property name="women"      							  type="string"><column name="women" /></property>
      
  	</class>
</hibernate-mapping>

