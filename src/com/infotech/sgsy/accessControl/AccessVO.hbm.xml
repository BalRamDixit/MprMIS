<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping> 

	<class name="com.infotech.sgsy.accessControl.AccessVO" table="AccessVO"> 
		<id name="id" column="roleId" >		
			<generator class="sequence">				
				<param name="sequence">role_id_seq</param>		
			</generator> 
		</id>		
 		<property name="roleName" column="roleName" /> 
 		<property name="roleDescription" column="roleDescription" /> 
 		<property name="levelRoleName" column="levelRoleName" /> 
 	</class>
</hibernate-mapping>